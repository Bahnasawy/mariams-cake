{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "jsx": "react",
    "strict": true,
    "baseUrl": "src",
    "paths": {
      "@components/*": ["components/*.tsx"],
      "@svgs/*": ["svgs/*.svg"],
      "@imgs/*": ["../static/images/*.png"]
    },
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
